{
  "name": "@bond-london/website-compare",
  "version": "1.3.1",
  "description": "Compare a website visually",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
